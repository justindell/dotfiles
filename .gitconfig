[user]
	name = Justin Dell
	email = dell.justin@gmail.com
[alias]
  w = whatchanged
  s = status -s
  co = checkout
  ci = commit
  st = status
  br = branch
  rs = reset
  c = commit -m
  df = diff
  di = diff
  dic = diff --cached
  a = add
  subtract = reset HEAD
  pl = pull
  plre = pull --rebase
  ps = push
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  lo = log --oneline --decorate
  last = log -1 HEAD
  out = log origin..HEAD
  pending = !git --no-pager log origin/master..master && echo
  missing = !sh -c 'git cherry "$0" | cut -sd + -f 2 | xargs "git show"'
[color]
  branch = auto
  diff = auto
  grep = auto
  interactive = auto
  status = auto
  ui = auto
[core]
  excludesfile = /home/$HOME/.gitignore
  editor = mate -w
  safecrlf = true
[apply]
  whitespace = nowarn
[push]
  default = tracking
[branch]
  autosetupmerge = always
